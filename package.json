{
	"name": "stream-feathers-nextjs-react-hooks-example",
	"description": "Example Streaming Application using Feathers with Next.js and React Hooks",
	"version": "0.1.0",
	"license": "MIT",
	"private": true,
	"repository": {
		"type": "git",
		"url": "https://gitlab.com/mjbrown/stream-feathers-nextjs-react-hooks-example"
	},
	"author": {
		"name": "Murray J Brown",
		"email": "mjb@mjbrown.com"
	},
	"contributors": [],
	"bugs": {
		"url": "https://gitlab.com/mjbrown/stream-feathers-nextjs-react-hooks-example/issues"
	},
	"main": "dist/server/index.js",
	"directories": {
		"client": "client",
		"server": "server",
		"test": "test"
	},
	"engines": {
		"node": ">= 11.9.0",
		"npm": ">= 6.9.0"
	},
	"scripts": {
		"build": "npm run build-styles && npm run build-utils && npm run build-server && npm run build-client",
		"build-client": "NODE_ENV='production' next build",
		"build-server": "NODE_ENV='production' babel ./src/server --out-dir ./dist/server",
		"build-utils": "NODE_ENV='production' babel ./src/utils --out-dir ./dist/utils",
		"build-styles": "node-sass src/styles/base.scss public/styles/base.css",
		"clean": "rimraf node_modules/.cache .next *.log",
		"debug": "npm run build-styles && npm run debug-server",
		"debug-server": "NODE_ENV='development' babel-watch -d -I -w ./src ./src/server/",
		"dev": "npm run build-styles && npm run dev-server",
		"dev-server": "NODE_ENV='development' babel-watch -D -w ./src -x ./src/client ./src/server/",
		"flow": "flow --include-warnings --show-all-errors --strip-root",
		"flow-typed": "flow-typed install",
		"kill": "pm2 kill",
		"lint": "npm run lint-src && npm run lint-pages",
		"lint:warn": "npm run lint-src:warn && npm run lint-pages:warn",
		"lint-src": "eslint --quiet src/",
		"lint-src:warn": "eslint src/",
		"lint-pages": "eslint --quiet pages/*",
		"lint-pages:warn": "eslint pages/*",
		"prebuild": "npm run flow-typed",
		"prebuild-client": "rimraf ./dist/client/ && mkdir -p ./dist/client",
		"prebuild-server": "rimraf ./dist/server/ && mkdir -p ./dist/server",
		"prestart": "npm run stop",
		"prestart-server": "npm run build",
		"purge": "npm run clean && rimraf .next data dist logs",
		"start": "npm run start-server",
		"start-server": "NODE_ENV='production' pm2 start ./dist/server/",
		"stop": "pm2 stop dist/server/",
		"styles": "npm run build-styles",
		"test": "npm run test-unit:js && npm run test-unit:jsx && npm run test-int",
		"test-int": "npm run start && cypress run",
		"test-int-dev": "concurrently 'NODE_ENV=\"testing\" babel-node src/server/' 'cypress open'",
		"test-unit": "npm run test-unit:js && npm run test-unit:jsx",
		"test-unit:js": "NODE_ENV='testing' ava",
		"test-unit:jsx": "NODE_ENV='testing' jest",
		"test-unit-all": "npm run test-unit-all:js && npm run test-unit-all:jsx",
		"test-unit-all:js": "NODE_ENV='testing' TESTSPEC='(spec|test)' ava",
		"test-unit-all:jsx": "NODE_ENV='testing' TESTSPEC='(spec|test)' jest",
		"test-unit-dev": "npm run test-unit-dev:js && npm run test-unit-dev:jsx",
		"test-unit-dev:js": "NODE_ENV='testing' TESTSPEC='test' ava",
		"test-unit-dev:jsx": "NODE_ENV='testing' TESTSPEC='test' jest --passWithNoTests",
		"trace": "NODE_ENV='development' node --trace-warnings dist/server/"
	},
	"dependencies": {
		"@feathersjs/authentication": "^2.1.16",
		"@feathersjs/authentication-jwt": "^2.0.10",
		"@feathersjs/authentication-local": "^1.2.9",
		"@feathersjs/client": "^3.7.8",
		"@feathersjs/configuration": "^2.0.6",
		"@feathersjs/errors": "^3.3.6",
		"@feathersjs/express": "^1.3.1",
		"@feathersjs/feathers": "^3.3.1",
		"@feathersjs/socketio": "^3.2.9",
		"@feathersjs/socketio-client": "^1.2.1",
		"@most/prelude": "^1.7.3",
		"@zeit/next-css": "^1.0.1",
		"@zeit/next-sass": "^1.0.1",
		"bulma": "^0.7.4",
		"compression": "^1.7.4",
		"config": "^3.1.0",
		"cors": "^2.8.5",
		"dayjs": "^1.8.15",
		"express": "^4.14.0",
		"feathers-hooks-common": "^4.20.7",
		"feathers-nedb": "^4.0.1",
		"helmet": "^3.16.0",
		"immer": "^8.0.1",
		"json-complete": "^2.0.0",
		"most": "^1.7.3",
		"nedb": "^1.8.0",
		"next": "^11.1.0",
		"next-router": "^1.3.6",
		"node-sass": "^4.12.0",
		"passport-custom": "^1.0.5",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-markdown": "^4.0.8",
		"react-redux": "^7.1.0",
		"redux": "^4.0.4",
		"redux-observable": "^1.1.0",
		"rust-match": "^0.1.7",
		"rxjs": "^6.5.2",
		"sass-convert": "^0.5.2",
		"sass-loader": "^7.1.0",
		"serve-favicon": "^2.5.0",
		"socket.io-client": "^2.2.0",
		"superagent": "^5.1.0",
		"winston": "^3.2.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.5.5",
		"@babel/core": "^7.5.5",
		"@babel/node": "^7.5.5",
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
		"@babel/plugin-proposal-optional-chaining": "^7.2.0",
		"@babel/preset-env": "^7.5.5",
		"@babel/preset-flow": "^7.0.0",
		"@babel/preset-react": "^7.0.0",
		"@babel/register": "^7.5.5",
		"@testing-library/jest-dom": "^4.0.0",
		"@testing-library/react": "^8.0.5",
		"@types/react": "^16.9.2",
		"autoprefixer": "^9.6.1",
		"ava": "^2.2.0",
		"babel-eslint": "^10.0.1",
		"babel-plugin-module-resolver": "^3.2.0",
		"babel-plugin-rewire": "^1.2.0",
		"babel-plugin-transform-remove-console": "^6.9.4",
		"babel-watch": "^7.0.0",
		"chai": "^4.2.0",
		"concurrently": "^4.1.1",
		"cypress": "^3.4.1",
		"eslint": "^6.1.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-config-prettier": "^6.0.0",
		"eslint-plugin-ava": "^7.1.0",
		"eslint-plugin-chai-friendly": "^0.4.1",
		"eslint-plugin-cypress": "^2.6.0",
		"eslint-plugin-fp": "^2.3.0",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jest": "^22.11.1",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.0",
		"eslint-plugin-react": "^7.14.3",
		"eslint-plugin-react-hooks": "^1.7.0",
		"esm": "^3.2.25",
		"flow-bin": "^0.102.0",
		"flow-typed": "^2.5.2",
		"jest": "^24.8.0",
		"jsdom": "^15.1.1",
		"jsdom-global": "^3.0.2",
		"normalize-scss": "^7.0.1",
		"pm2": "^3.5.1",
		"prettier": "^1.17.0",
		"react-test-renderer": "^16.8.6",
		"rewire": "^4.0.1",
		"rimraf": "^2.6.2",
		"sinon": "^7.3.2",
		"webpack": "^4.29.0",
		"webpack-cli": "^3.3.5",
		"webpack-dev-middleware": "^3.6.0",
		"webpack-hot-middleware": "^2.24.3"
	}
}
